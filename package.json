{
  "name": "fastify-prisma",
  "version": "1.0.0",
  "description": "Basic implementation of Prisma with Fastify",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "deploy": "npm install && npm run build && npm run db:migrate:prod",
    "test": "dotenv -e .env.test -- jest",
    "db:save": "prisma migrate dev --create-only --preview-feature",
    "db:migrate": "prisma migrate dev --preview-feature",
    "db:migrate:prod": "prisma migrate deploy --preview-feature",
    "db:seed": "prisma db seed --preview-feature",
    "postinstall": "husky install"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@prisma/client": "5.7.1",
    "fastify": "4.25.2"
    },
  "devDependencies": {
    "@types/jest": "29.5.11",
    "@types/node": "20.10.5",
    "@types/nodemon": "1.19.6",
    "@types/prettier": "2.7.3",
    "@types/supertest": "6.0.2",
    "dotenv-cli": "7.3.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lint-staged": "15.2.0",
    "nodemon": "3.0.2",
    "prettier": "3.1.1",
    "prisma": "5.7.1",
    "supertest": "6.3.3",
    "ts-eager": "2.0.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "typesync": "0.12.0",
    "unique-names-generator": "4.7.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write"
  }
}
